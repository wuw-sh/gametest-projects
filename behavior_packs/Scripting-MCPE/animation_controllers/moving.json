{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.moving": {
      "states": {
        "default": {
          "transitions": [
            {
              "moving": "query.is_moving"
            }
          ]
        },
        "moving": {
          "on_entry": [
            "@s moving"
          ]
        }
      }
    }
  }
}